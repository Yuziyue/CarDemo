    <FrameLayout
        android:layout_width="fill_parent"
        android:layout_height="fill_parent"
        tools:ignore="MissingConstraints">

        <ImageView
            android:layout_width="fill_parent"
            android:layout_height="fill_parent"
            android:src="@drawable/background"
            android:scaleType = "fitStart"
            />

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="vertical">

            <ImageView
                android:id="@+id/back_main_img"
                android:layout_width="20dp"
                android:layout_height="20dp"
                android:layout_marginLeft="20dp"
                android:layout_marginTop="25dp"
                android:src="@mipmap/back_" />

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:gravity="center_horizontal"
                android:orientation="vertical">
                <com.example.demo.view.CircleImageView
                    android:layout_width="120dp"
                    android:layout_height="120dp"
                    android:layout_marginTop="60dp"
                    android:src="@drawable/c"/>
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal"
                    android:gravity="center">

                    <TextView
                        android:layout_marginTop="10dp"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:textStyle="bold"
                        android:text="176****5771"
                        android:textSize="20sp"/>
                    <ImageView
                        android:layout_width="20dp"
                        android:layout_height="20dp"
                        android:layout_marginTop="10dp"
                        android:src="@mipmap/edit_" />
                </LinearLayout>
                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:textSize="20sp"
                    android:layout_marginTop="10dp"
                    android:text="个人中心"/>

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="40dp"
                        android:gravity="center"
                        android:orientation="horizontal">

                        <Button
                            android:layout_width="141dp"
                            android:layout_height="wrap_content"
                            android:layout_gravity="center_horizontal"
                            android:layout_margin="15dp"
                            android:background="@drawable/background_button_user"
                            android:drawableLeft="@mipmap/account_"
                            android:drawableRight="@mipmap/back_1"
                            android:drawablePadding="1sp"
                            android:paddingStart="20dp"
                            android:paddingRight="40dp"
                            android:text="我的账单"
                            android:textColor="@drawable/button_font_style_user" />

                        <Button
                            android:layout_width="141dp"
                            android:layout_height="wrap_content"
                            android:layout_gravity="center_horizontal"
                            android:layout_margin="15dp"
                            android:background="@drawable/background_button_user"
                            android:drawableLeft="@mipmap/bind_"
                            android:drawableRight="@mipmap/back_1"
                            android:drawablePadding="1sp"
                            android:paddingStart="20dp"
                            android:paddingRight="40dp"
                            android:text="绑卡有礼"
                            android:textColor="@drawable/button_font_style_user" />

                    </LinearLayout>


                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:gravity="center"
                    android:orientation="horizontal">

                    <Button
                        android:id="@+id/charge_btn"
                        android:layout_width="141dp"
                        android:layout_height="wrap_content"
                        android:layout_gravity="center_horizontal"
                        android:layout_margin="15dp"
                        android:layout_marginTop="10dp"
                        android:background="@drawable/background_button_user"
                        android:drawableLeft="@mipmap/battery_"
                        android:drawableRight="@mipmap/back_1"
                        android:drawablePadding="1sp"
                        android:paddingStart="20dp"
                        android:paddingRight="40dp"
                        android:text="充电"
                        android:textColor="@drawable/button_font_style_user" />

                    <Button
                        android:layout_width="141dp"
                        android:layout_height="wrap_content"
                        android:layout_gravity="center_horizontal"
                        android:layout_margin="15dp"
                        android:layout_marginTop="10dp"
                        android:background="@drawable/background_button_user"
                        android:drawableLeft="@mipmap/park_"
                        android:drawableRight="@mipmap/back_1"
                        android:drawablePadding="1sp"
                        android:paddingStart="20dp"
                        android:paddingRight="40dp"
                        android:text="停车"
                        android:textColor="@drawable/button_font_style_user" />

                </LinearLayout>

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:gravity="center"
                    android:orientation="horizontal">

                    <Button
                        android:layout_width="141dp"
                        android:layout_height="wrap_content"
                        android:layout_gravity="center_horizontal"
                        android:layout_margin="15dp"
                        android:layout_marginTop="10dp"
                        android:background="@drawable/background_button_user"
                        android:drawableLeft="@mipmap/add_"
                        android:drawableRight="@mipmap/back_1"
                        android:drawablePadding="1sp"
                        android:paddingStart="20dp"
                        android:paddingRight="40dp"
                        android:text="加油"
                        android:textColor="@drawable/button_font_style_user" />

                    <Button
                        android:layout_width="141dp"
                        android:layout_height="wrap_content"
                        android:layout_gravity="center_horizontal"
                        android:layout_margin="15dp"
                        android:layout_marginTop="10dp"
                        android:background="@drawable/background_button_user"
                        android:drawableLeft="@mipmap/highspeed_"
                        android:drawableRight="@mipmap/back_1"
                        android:drawablePadding="1sp"
                        android:paddingStart="20dp"
                        android:paddingRight="40dp"
                        android:text="高速"
                        android:textColor="@drawable/button_font_style_user" />

                </LinearLayout>
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:gravity="center"
                    android:orientation="horizontal">

                    <Button
                        android:layout_width="141dp"
                        android:layout_height="wrap_content"
                        android:layout_gravity="center_horizontal"
                        android:layout_margin="15dp"
                        android:layout_marginTop="10dp"
                        android:background="@drawable/background_button_user"
                        android:drawableLeft="@mipmap/driver_"
                        android:drawableRight="@mipmap/back_1"
                        android:drawablePadding="1sp"
                        android:paddingStart="20dp"
                        android:paddingRight="40dp"
                        android:text="代驾"
                        android:textColor="@drawable/button_font_style_user" />

                    <Button
                        android:layout_width="141dp"
                        android:layout_height="wrap_content"
                        android:layout_gravity="center_horizontal"
                        android:layout_margin="15dp"
                        android:layout_marginTop="10dp"
                        android:background="@drawable/background_button_user"
                        android:drawableLeft="@mipmap/around_"
                        android:drawableRight="@mipmap/back_1"
                        android:drawablePadding="1sp"
                        android:paddingStart="20dp"
                        android:paddingRight="40dp"
                        android:text="附近"
                        android:textColor="@drawable/button_font_style_user" />

                </LinearLayout>


            </LinearLayout>



        </LinearLayout>


    </FrameLayout>



</androidx.constraintlayout.widget.ConstraintLayout>
